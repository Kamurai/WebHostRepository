@{
    Layout = "~/Layouts/Section1/Section6/_Layout.cshtml";
    Page.Title = "Oracle Programming";

    var db = Database.Open("OracleConnectionString");
    var selectQueryString = "SELECT * FROM Test";
}

<p align="left">
	This section is dedicated to Oracle programming.
</p>

<table> 
    <tr>
        <td>
            Oracle Instance  
        </td>
        <td></td>
    </tr>
    <tr>
        <th>Index</th> 
        <th>Color</th> 
    </tr>
    @foreach(var row in db.Query(selectQueryString))
    {
        <tr> 
            <td>@row.indext</td> 
            <td>@row.Color</td> 
        </tr> 
    }
</table>